<assembly>
   <id>jaxws</id>
   <formats>
      <format>jar</format>
   </formats>
   <includeBaseDirectory>false</includeBaseDirectory>
   <fileSets>
      <fileSet>
         <directory>target/classes</directory>
         <outputDirectory>/</outputDirectory>
         <includes>
            <include>**/*.class</include>
            <include>**/*.wsdl</include>
         </includes>
      </fileSet>
      
       <fileSet>
         <directory>target/generated</directory>
         <outputDirectory>/</outputDirectory>
         <includes>
            <include>**/*.class</include>
            <include>**/*.wsdl</include>
         </includes>
      </fileSet>
      
   </fileSets>
</assembly>